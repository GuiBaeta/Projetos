package br.gbaeta.com.application;

import br.gbaeta.com.chess.ChessPiece;

public class UI {

	public static void printBoard(ChessPiece[][] pieces) {
		
	}
	
	private static void printPiece(ChessPiece piece) {
		if (piece == null) {
			System.out.println("-");
		} else {
			System.out.println(piece);
		}
		System.out.println(" ");
	}
}
